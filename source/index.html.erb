---
title: TIY Houston Announcements
---
<div class="reveal">
<!-- Any section element inside of this container is displayed as a slide -->
  <div class="slides">
<!-- Welcome Slide -->
		<section>
			<h2><div id='today'></div></h2>
			<%= image_tag "logo_big.png" %>
		</section>
<!-- About TIY -->
    <section>
      <%= partial "about" %>
    </section>
<!-- Students -->
    <section>
      <section>
        <h2>About The Students</h2>
      </section>
      <% demo.each do |demo| %>
          <%= partial "demo", locals: {demo: demo} %>
      <% end %>
    </section>
<!-- Announcements, if any  -->
    <% unless this_week_announcements.empty? %>
    <section>
      <h3>Announcements</h3>
      <table>
        <% this_week_announcements.each do |announcement| %>
            <%= partial "announcements", locals: {announcement: announcement} %>
        <% end %>
      </table>
    </section>
    <% end %>
<!-- Meetups @ TIY this week, if any -->
<% unless meetups_this_week.empty? %>
  <section>
  <h3>Meetups @TIY This Week</h3>
    <ul>
      <% meetups_this_week.each do |meetup| %>
        <li><h4><%= meetup.group.name %> - <%= Time.at(meetup.time/1000).strftime('%a, %b %d - %l:%M%p') %> - <em>"<%= meetup.name %>"</em></h4></li>
      <% end %>
    </ul>
    </section>
<% end %>
<!-- Meetups worth going this week, if any -->
    <% unless meetups_worth_this_week.empty? %>
      <% meetups_worth_this_week.each do |meetup| %>
          <%= partial "meetupworthit", locals: {meetup: meetup} %>
      <% end %>
    <% end %>
<!-- Guest Speakers, if any -->
    <% unless this_week_speaker.empty? %>
      <% this_week_speaker.each do |speaker| %>
          <%= partial "speaker", locals: {speaker: speaker} %>
      <% end %>
    <% end %>
<!-- Wifi -->
    <section>
      <%= image_tag "wifi.png", :class => "rounded" %>
    </section>
<!-- Comics -->
    <section>
      <%= image_tag comic_url, :class => "rounded" %>
    </section>
  </div>
</div>
